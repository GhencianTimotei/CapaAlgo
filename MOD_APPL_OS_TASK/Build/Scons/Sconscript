Import('env_application')

objects = []
                            
#==============================================================================#
# Application modules
#==============================================================================#
env_app = env_application.Clone()
                            
appl_sources = [
				'../../Source/Os_Stubs.c'
]
                            
objects += env_app.Object(appl_sources)
env_app['CCFLAGS'].remove('-Om')
add_CCFLAGS_01 = ['-Ohs']
env_app.Append(CCFLAGS=add_CCFLAGS_01)
                            
                            
#==============================================================================#
# Test suites
#==============================================================================#
                            
                            
                            
#==============================================================================#
# Return objects
#==============================================================================#
Return('objects')